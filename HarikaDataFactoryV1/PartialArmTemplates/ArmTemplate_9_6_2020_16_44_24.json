{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HarikaDataFactoryV1"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipelinefromgithub')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "copy data from github",
				"activities": [
					{
						"name": "ValidationonName",
						"type": "Validation",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"dataset": {
								"referenceName": "ds_source1",
								"type": "DatasetReference"
							},
							"timeout": "7.00:00:00",
							"sleep": 10
						}
					}
				],
				"parameters": {
					"appendeddata1": {
						"type": "string",
						"defaultValue": "appendeddata1"
					},
					"appendeddata2": {
						"type": "string",
						"defaultValue": "appendeddata2"
					},
					"appendeddata3": {
						"type": "string",
						"defaultValue": "appendeddata3"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		}
	]
}